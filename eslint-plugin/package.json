{
  "name": "eslint-plugin-lambda-html",
  "publishConfig": {
    "access": "public"
  },
  "version": "1.2.0",
  "description": "ESLint plugin for lambda.html - lint rules for setClass() including known modifiers, spacing, duplicates, conflicts, and empty calls",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "USAGE.md"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "test": "node test/rule.test.js",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/seckmaster/lambda.html.git",
    "directory": "eslint-plugin"
  },
  "bugs": {
    "url": "https://gitlab.com/seckmaster/lambda.html/issues"
  },
  "homepage": "https://gitlab.com/seckmaster/lambda.html/tree/main/eslint-plugin#readme",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "lambda.html",
    "tailwind",
    "linter",
    "code-quality"
  ],
  "author": "Toni K. Turk",
  "license": "ISC",
  "peerDependencies": {
    "eslint": ">=7.0.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.0.0",
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
