{
  "name": "eslint-plugin-fluent-html",
  "publishConfig": {
    "access": "public"
  },
  "version": "1.4.0",
  "description": "ESLint plugin for fluent-html - lint rules for setClass, variadic children, HTMX best practices, type-safe IDs, and more",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
